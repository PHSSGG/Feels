<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.Feels" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/purple_primary</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/white</item>
    </style>

    <style name="Theme.Feels.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Feels.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Feels.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>